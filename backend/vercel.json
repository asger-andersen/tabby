{
    "version": 2,
    "builds": [
        {
            "src": "api/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/app.js"
        }
    ],
    "functions": {
        "api/app.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}